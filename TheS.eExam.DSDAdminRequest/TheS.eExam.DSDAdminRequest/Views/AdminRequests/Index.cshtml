@model IEnumerable<TheS.eExam.DSDAdminRequest.DSA.EF.AdminRequest>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
